/include/ "skeleton.dtsi"

/ {
	compatible = "samsung,exynos5250";

	sromc@12250000 {
		compatible = "samsung,exynos-sromc";
		reg = <0x12250000 0x20>;
		#address-cells = <1>;
		#size-cells = <0>;
	};
};
