/dts-v1/;
/include/ "uniphier-ph1-pro4.dtsi"

/ {
	model = "Panasonic UniPhier PH1-Pro4 Reference Board";
	compatible = "panasonic,ph1-pro4-ref", "panasonic,ph1-pro4";

	memory {
		device_type = "memory";
		reg = <0x80000000 0x40000000>;
	};

	chosen {
		bootargs = "console=ttyPS0,115200 earlyprintk";
	};
};
